<?xml version="1.0" encoding="UTF-8"?>
<web-app>

		<servlet>
			<servlet-name>login</servlet-name>
			<servlet-class>info.LoginData</servlet-class>	
		</servlet>
		
		<servlet>
			<servlet-name>profile</servlet-name>
			<servlet-class>info.ProfileShow</servlet-class>	
		</servlet>
		
		<servlet>
			<servlet-name>logout</servlet-name>
			<servlet-class>info.LogoutData</servlet-class>	
		</servlet>
		
		<servlet-mapping>
			<servlet-name>logout</servlet-name>
			<url-pattern>/logout</url-pattern>
		</servlet-mapping>
		
		<servlet-mapping>
			<servlet-name>profile</servlet-name>
			<url-pattern>/profile</url-pattern>
		</servlet-mapping>
		
		<servlet-mapping>
			<servlet-name>login</servlet-name>
			<url-pattern>/login</url-pattern>
		</servlet-mapping>
	
</web-app>